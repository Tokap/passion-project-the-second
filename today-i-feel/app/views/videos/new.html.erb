<h1>Add a new video</h1>

<%= form_for :video, url: videos_path, method: :post do |f| %>
  <p class="form-tags">Name: </p> <%= f.text_field :name %><br />
  <p class="form-tags url">Url: </p><%= f.text_field :embedded_url, :required => true %><br />
  <p class="form-tags">Emotion: </p><%= select_tag(:emotion, options_for_select([['Happy'], ['Sad'], ['Numb'], ['Angry'], ['Lost']])) %><br /><br />
  <%= f.submit %>
<% end %>