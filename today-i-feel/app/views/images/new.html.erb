<h1>Add a new image</h1>

<%= form_for :image, url: images_path, method: :post do |f| %>
  Name: <%= f.text_field :name %><br />
  Url: <%= f.text_field :url, :required => true %><br />
  Emotion: <%= select_tag(:emotion, options_for_select([['Happy'], ['Sad'], ['Numb'], ['Angry'], ['Lost']])) %><br /><br />
  <%= f.submit %>
<% end %>