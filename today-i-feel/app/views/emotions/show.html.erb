<h1 class="emotion-header"> <%=@emotion.name %> </h1>

<section class="quote-display hidden">
  <% @quotes.each do |quote| %>
     <p class="quote-body"> <%= quote.body %> </p>
     <p class="quote-author"><%= link_to "#{quote.author}", Wikipedia.find(quote.author).fullurl %></p>
  <% end %>
</section>

<section class="video">
  <% @videos.each do |video| %>
    <iframe width="250" height="215" src="<%=video.embedded_url%>" frameborder="0" allowfullscreen></iframe>
    <% end %>
</section>

<section class="images">
  <%= image_tag("https://secure.static.tumblr.com/121be077c6bf3e5fc765b952c290f38a/sbwi74e/eEJnu0ljw/tumblr_static_tumblr_static_7tpcdqn9vx8gg8o80ws0scwss_640.jpg", size: "400x250", alt: "Pretty Picture") %>
</section>